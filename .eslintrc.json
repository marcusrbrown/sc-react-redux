{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "parser": "@babel/eslint-parser",
  "rules": {
    "indent": ["error", 2, {"SwitchCase": 1}],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "comma-dangle": 0,
    "no-console": 0,
    "max-len": ["error", 120, 2, {"ignoreComments": true}],
    "func-names": 0,
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}]
  }
}
